{
  "builds": [
    {
      "src": "rapidCare",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "rapidCare/vercel_handler.py",
      "headers": { "script": "auto" }
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "rapidCare.settings"
  }
}
